{% extends "base.html.twig" %}
{% block title %}RCL | Next Bands{% endblock %}
{% block headerText %}Next {{ displayOptions.upcomingCount }} Bands{% endblock %}
{% block containerClass %}upcoming{% endblock %}

    {% block pageJs %}
        <script>
            var displayOptions = {{ displayOptions|json_encode|raw }};
            $(function() {
                ticketer.displayOptions = displayOptions;
                ticketer.go();
                {% if not is_granted('ROLE_ADMIN') %}
                ticketer.checkRemoteRedirect();
                {% endif %}
            });
        </script>
    {% endblock %}

