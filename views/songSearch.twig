<html>
<head>
    <title>Song Search</title>
    <script src="/components/jquery/jquery.min.js"></script>
    <script src="/components/handlebars/handlebars.js"></script>
    <script src="js/ticket.js"></script>
    <script>
        var displayOptions = {{ displayOptions|json_encode|raw }};
        $(function () {
            ticketer.displayOptions = displayOptions;
            ticketer.initSearchPage();
        });
    </script>
    <link rel="stylesheet" href="/components/bootstrap/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="/css/ticket.css"/>
    <link rel="stylesheet" href="/components/font-awesome/css/font-awesome.min.css"/>
</head>
<body>
<div class="container songSearch">
    <div class="page-header well"><p class="text-center">Search songs</p>
    </div>

    <div class="col-md-6">
        <div class="input-group input-group">
            <span class="input-group-addon" id="search-addon1"><span class="fa fa-search"></span> </span>
            <input class="searchString form-control" placeholder="Search song or use code"/>
        </div>

        <div class="songCompleteOuter">
            <div class="songComplete"></div>
        </div>
    </div>

    <div class="col-md-6">
        <div id="target"></div>
    </div>
</div>


<div class="end">
    <div class="gflink page-footer"><p class="text-center">gofundme.com/rcl2015</p></div>
</div>
</body>
</html>